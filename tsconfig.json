{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "declaration": false,
    "module": "esnext",
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "lib": [
      "es2017",
      "es2016",
      "dom",
      "es5"
    ]
  },
  "exclude": [
    "**/node_modules/**/*"
  ]
}
